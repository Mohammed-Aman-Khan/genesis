{
  "name": "@genesis/plugins",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./node": {
      "import": "./dist/plugins/node/index.js",
      "types": "./dist/plugins/node/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts src/plugins/node/index.ts --dts --format esm --minify",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts",
    "test": "vitest"
  },
  "dependencies": {
    "@genesis/core": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.6.0",
    "tsup": "^8.0.0",
    "eslint": "^9.0.0",
    "@types/node": "^22.0.0",
    "vitest": "^2.0.0"
  }
}
